<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="website_assets" inherit_id="website.assets_frontend">
		<xpath expr="link[last()]" position="after">
			<link href="https://fonts.googleapis.com/css?family=Tangerine" rel="stylesheet" type="text/css" />
			<link href="/theme_tutorial/static/src/css/theme_style.css" rel="stylesheet" type="text/css"/>
			<link href="/theme_tutorial/static/src/css/w3.css" rel="stylesheet" type="text/css"/>
		</xpath>

<!-- script[last()] -->
		<xpath expr="." position="inside">
    		<script type="text/javascript" src="/theme_tutorial/static/src/js/style.js" />
  		</xpath>
	</template>
</odoo>
